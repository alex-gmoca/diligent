{
  "family": "custom-code-runner-task",
  "containerDefinitions": [
    {
      "name": "code-runner",
      "image": "12345.dkr.ecr.us-west-2.amazonaws.com/custom-code-runner",
      "memory": 512,
      "cpu": 256,
      "environment": [
        { "name": "CODE_S3_BUCKET", "value": "custom-code-bucket" },
        { "name": "SCRIPT_ID", "value": "" }
      ],
      "essential": true
    }
  ],
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512"
}
